{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "preserve"
  },
  "paths": {
    "@component/*": ["./component/*"],
    "@assets/*": ["./assets/*"],
    "@apis/*": ["./apis/*"],
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
